{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/modal.jsx",
		"app/_layout.jsx",
		"app/(tabs)/event.jsx",
		"app/(tabs)/index.jsx",
		"app/(tabs)/_layout.jsx",
		"constants/index.js",
		"tamagui.config.ts"
	]
}
