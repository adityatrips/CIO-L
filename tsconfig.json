{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"app/**/*.jsx",
		"app/(tabs)/**/*.jsx",
		"app/(authUser)/**/*.jsx",
		"constants/index.js",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"tamagui.config.ts"
	]
}
